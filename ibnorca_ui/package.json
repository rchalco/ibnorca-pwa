{
  "name": "ibnorca_ui",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron:mac": "electron-packager ./electron sigad --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "electron:win": "electron-packager ./electron sigad --overwrite --asar=true --platform=win32"
  },
  "private": true,
