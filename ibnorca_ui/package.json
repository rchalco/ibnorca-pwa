    "@ionic-native/core": "^5.31.1",
    "@ionic-native/document-viewer": "^4.20.0",
    "@ionic-native/file": "^5.31.1",
    "@ionic-native/file-transfer": "^5.31.1",
    "@ionic-native/splash-screen": "^5.31.1",
    "@ionic-native/status-bar": "^5.31.1",
